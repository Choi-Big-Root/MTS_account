<?xml version="1.0" encoding="UTF-8"?>
<STRUCT CODE="REALTEN" SERVICECODE="REALTEN" NAME="실시간 시세 관련">
	<BLOCK CODE="PageBlock" NAME="PageBlock" SEND="NO" ATTR="NO" OCCURS="NO">
		<FIELD CODE="code" TYPE="CHAR" SIZE="16" NAME="realtime code"/>
		<FIELD CODE="pind" TYPE="CHAR" SIZE="1" NAME="price indicator"/>
		<FIELD CODE="ccol" TYPE="CHAR" SIZE="1" NAME="last price color"/>
		<FIELD CODE="last" TYPE="CHAR" SIZE="9" NAME="last price"/>
		<FIELD CODE="bidp" TYPE="CHAR" SIZE="9" NAME="bid price"/>
		<FIELD CODE="btim" TYPE="CHAR" SIZE="9" NAME="bid time"/>
		<FIELD CODE="askp" TYPE="CHAR" SIZE="9" NAME="ask price"/>
		<FIELD CODE="atim" TYPE="CHAR" SIZE="9" NAME="ask time"/>
		<FIELD CODE="sign" TYPE="CHAR" SIZE="1" NAME="net change sign(+/-)"/>
		<FIELD CODE="diff" TYPE="CHAR" SIZE="9" NAME="net change"/>
		<FIELD CODE="rate" TYPE="CHAR" SIZE="3" NAME="% chang"/>
		<FIELD CODE="open" TYPE="CHAR" SIZE="9" NAME="open price"/>
		<FIELD CODE="high" TYPE="CHAR" SIZE="9" NAME="high price"/>
		<FIELD CODE="lowp" TYPE="CHAR" SIZE="9" NAME="low price"/>
		<FIELD CODE="time" TYPE="CHAR" SIZE="6" NAME="trading time"/>
		<FIELD CODE="tday" TYPE="CHAR" SIZE="8" NAME="trading date"/>
	</BLOCK>
</STRUCT>


